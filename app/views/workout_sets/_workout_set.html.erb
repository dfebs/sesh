<%= turbo_frame_tag combined_dom_id(workout_set.workout_instance, workout_set) do %>
  <li> 
    <%= workout_set.reps %> rep(s) of <%= workout_set.amount %> <%= workout_set.preferred_unit %>
    <%= button_to "Delete Reps", workout_set_path(workout_set), method: :delete %>
    <%= link_to "Edit", edit_workout_set_path(workout_set) %>
  </li>
<% end %>
