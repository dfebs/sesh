<li>
  <%= turbo_frame_tag combined_dom_id(workout_set.workout_instance, workout_set) do %>
      <%= workout_set.reps %> rep(s) of <%= workout_set.amount %> <%= workout_set.preferred_unit %>
      <% if !workout_set.workout_session.completed? %>
        <%= button_to "Delete", workout_set_path(workout_set), method: :delete, class: "red-button" %>
        <%= link_to "Edit", edit_workout_set_path(workout_set), class: "basic-button" %>
      <% end %>
  <% end %>
</li>
