<%= form_with model: workout_session do |form| %>
  <%= form.label :date_completed %>
  <%= form.date_select :date_completed, end_year: Date.today.year, start_year: 2000, selected: Date.today %>

  <%= form.button class: "green-button" do %>
    <%= render "icons/check" %>
    <span>Complete</span>
  <% end %>
<% end %>
